package open3d.geom
{
	/**
	 * Vertex
	 * @author katopz
	 * 
	 */	
	public class Vertex
	{
		public var x:Number;
		public var y:Number;
		public var z:Number;
		public var w:Number;
		
		public function Vertex(x:Number=0, y:Number=0, z:Number=0, w:Number=0)
		{
			this.x=x;
			this.y=y;
			this.z=z;
			this.w=w;
		}
	}
}
